{
  "extends": [
    "airbnb"
  ],
  "plugins": ["react"],
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": 0,
    "space-before-function-paren": [1, "always"],
    "jsx-quotes": [1, "prefer-single"]
  }
}
